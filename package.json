{
    "name": "@beautybox/core",
    "version": "1.0.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git@gitlab.beautybox.ru:beautybox/frontend/beautybox-base.git"
    },
    "main": "config.js",
    "license": "MIT",
    "publishConfig": {
        "access": "restricted"
    },
    "scripts": {
        "test": "jest -u",
        "test:watch": "jest -u --watch",
        "generate-docs": "jsdoc ./helpers -c jsdoc.json -r"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,vue}": "eslint --fix --config ./.eslintrc --ignore-path ./.eslintignore",
        "*.{scss,sass}": [
            "stylelint --fix --config ./.stylelintrc",
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^26.6.3",
        "babel-plugin-require-context-hook": "^1.0.0",
        "docdash": "^1.2.0",
        "eslint": "^7.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "jest": "^26.6.3",
        "jsdoc": "^3.6.6",
        "stylelint": "^13.7.0",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.0.0",
        "stylelint-scss": "^3.16.0",
        "stylelint-selector-bem-pattern": "^2.1.0",
        "stylelint-webpack-plugin": "^2.0.0",
        "vue-jest": "^3.0.7",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "babel-loader": "^8.2.1",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.1",
        "fibers": "^5.0.0",
        "mini-css-extract-plugin": "^0.11.0",
        "node-fetch": "^2.6.1",
        "preload-webpack-plugin": "3.0.0-beta.3",
        "sass": "^1.26.11",
        "sass-loader": "^10.0.5",
        "script-ext-html-webpack-plugin": "^2.1.4",
        "socket.io-client": "^2.3.0",
        "style-loader": "^2.0.0",
        "svg-sprite-loader": "^5.0.0",
        "vue": "^2.6.12",
        "vue-loader": "^15.9.3",
        "vue-socket.io": "^3.0.9",
        "vue-toasted": "^1.1.28",
        "vuelidate": "^0.7.5",
        "vuetify": "^2.3.18",
        "vuetify-loader": "^1.6.0",
        "vuex": "^3.6.0",
        "webpack": "^4.44.2"
    },
    "jest": {
        "collectCoverage": true,
        "transform": {
            "^.+\\.js$": "babel-jest",
            "^.+\\.vue$": "vue-jest"
        },
        "transformIgnorePatterns": [],
        "moduleNameMapper": {
            "^.+\\.(css|less|scss|sass|svg)$": "babel-jest"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "moduleDirectories": [
            "node_modules"
        ]
    }
}
